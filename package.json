{
  "name": "TravelExpenses",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^4.1.0",
    "@sap-cloud-sdk/http-client": "^4.1.0",
    "@sap/cds-common-content": "^3.0.1",
    "@sap/xssec": "^4",
    "express": "^4",
    "form-data": "4.0.4"
  },
  "engines": {
    "node": ">=20"
  },
  "resolutions": {
    "form-data": "4.0.4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9.2.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-travelexpense-ui": "cds watch --open travelexpense-ui/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy TravelExpenses --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "destination": {
        "kind": "destination",
        "vcap": {
          "label": "destination",
          "plan": "lite"
        }
      },
      "[production]": {
        "auth": "xsuaa",
        "db": "hana"
      },
      "destinations": true,
      "html5-runtime": true,
      "workzone": true
    }
  }
}
